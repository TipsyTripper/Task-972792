CREATE TABLE audit_department
(
    Ревизионный_номер VARCHAR(7) not null ,
    Тип_изделия VARCHAR(50) not null ,
    Адрес_расположения varchar(50) not null
);

INSERT INTO audit_department
("Ревизионный_номер", "Тип_изделия", "Адрес_расположения")
VALUES ('Xg_5124', 'Компьютер', 'Общага'),
       ('Ng_7634', 'Тостер', 'Кабинет_426'),
       ('Tj_6668', 'Электроплита', 'Подвал');

select *
from audit_department;